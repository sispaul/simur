	<queryString>
		<![CDATA[SELECT objeto_opcion.ide_obop,
opcion.nom_opci,
objeto_opcion.nom_obop,
objeto_opcion.descripcion_obop,
PERFIL_OBJETO.LECTURA_PEOB,
