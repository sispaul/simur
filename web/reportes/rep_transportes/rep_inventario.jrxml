	<queryString>
		<![CDATA[SELECT DISTINCT p.NUMERO,
p.PLACA,t.FECHA_ENVIO_ACTA,
t.FECHA_REGISTRO_ACTA,
t.ANO,t.NUMERO_ACTA,
t.ENTREGADO_ACTA,t.RECIBIDO_ACTA,
p.IDE_PLACA,s.DESCRIPCION_SERVICIO,
v.des_tipo_vehiculo,s.IDE_TIPO_SERVICIO,
v.ide_tipo_vehiculo,t.IDE_INGRESO_PLACAS,
e.DESCRIPCION_ESTADO,r.DESCRIPCION_PLACA
FROM
dbo.TRANS_PLACA AS p ,
dbo.TRANS_INGRESOS_PLACAS AS t ,
dbo.trans_tipo_vehiculo AS v ,
dbo.TRANS_TIPO_SERVICIO AS s ,
dbo.TRANS_TIPO_ESTADO AS e,
dbo.TRANS_TIPO_PLACA r
WHERE
p.IDE_INGRESO_PLACAS = t.IDE_INGRESO_PLACAS AND
p.IDE_TIPO_VEHICULO = v.ide_tipo_vehiculo AND
s.IDE_TIPO_VEHICULO = v.ide_tipo_vehiculo AND
p.IDE_TIPO_SERVICIO = s.IDE_TIPO_SERVICIO AND
p.IDE_TIPO_ESTADO = e.IDE_TIPO_ESTADO AND
p.IDE_TIPO_PLACA = r.IDE_TIPO_PLACA AND
t.NUMERO_ACTA = $P{acta} AND
t.FECHA_ENVIO_ACTA = cast ($P{fecha_envio} as date)
ORDER BY p.PLACA ASC]]>
	</queryString>
