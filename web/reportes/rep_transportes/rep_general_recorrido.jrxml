	<queryString>
		<![CDATA[select * from
(select a.*,c.kilometros from
(SELECT PLACA_VEHICULO,NUMERO_ABASTECIMIENTO
FROM MVABASTECIMIENTO_COMBUSTIBLE
where ANIO = $P{anio} and PERIODO = $P{periodo})as a
left join
(SELECT PLACA_VEHICULO,ROUND((KILOMETRAJE),2) as kilometros,NUMERO_ABASTECIMIENTO
FROM MVABASTECIMIENTO_COMBUSTIBLE
GROUP BY KILOMETRAJE,PLACA_VEHICULO,NUMERO_ABASTECIMIENTO) as c
on a.placa_vehiculo = c.placa_vehiculo and a.numero_abastecimiento = c.numero_abastecimiento
) as p
order by numero_abastecimiento]]>
	</queryString>
